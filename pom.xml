            <version>30.1.1-jre</version>
        </dependency>
    </dependencies>
    <distributionManagement>
        <repository>
            <id>fpsoknad-mottak</id>
            <name>Github navikt Maven Packages</name>
            <url>https://maven.pkg.github.com/navikt/fpsoknad-mottak</url>
        </repository>
    </distributionManagement>
</project>

